{
  "version": 1,
  "projects": {
    "counter": {
      "projectType": "application",
      "root": "apps/counter",
      "sourceRoot": "apps/counter/src",
      "prefix": "counter",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/counter",
            "index": "apps/counter/src/index.html",
            "main": "apps/counter/src/main.ts",
            "polyfills": "apps/counter/src/polyfills.ts",
            "tsConfig": "apps/counter/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/counter/src/favicon.ico",
              "apps/counter/src/assets"
            ],
            "styles": [
              "apps/counter/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/counter/src/environments/environment.ts",
                  "with": "apps/counter/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "counter:build:production"
            },
            "development": {
              "browserTarget": "counter:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "counter:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/counter/src/**/*.ts",
              "apps/counter/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/counter"
          ],
          "options": {
            "jestConfig": "apps/counter/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "domain": {
      "projectType": "library",
      "root": "libs/domain",
      "sourceRoot": "libs/domain/src",
      "prefix": "domain",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": [
            "dist/libs/domain"
          ],
          "options": {
            "project": "libs/domain/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/domain/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/domain/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/domain"
          ],
          "options": {
            "jestConfig": "libs/domain/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/domain/src/**/*.ts",
              "libs/domain/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-state": {
      "projectType": "library",
      "root": "libs/shared-state",
      "sourceRoot": "libs/shared-state/src",
      "prefix": "shared-state",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared-state"
          ],
          "options": {
            "jestConfig": "libs/shared-state/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-state/src/**/*.ts",
              "libs/shared-state/src/**/*.html"
            ]
          }
        }
      }
    },
    "shop": {
      "projectType": "application",
      "root": "apps/shop",
      "sourceRoot": "apps/shop/src",
      "prefix": "shop",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/shop",
            "index": "apps/shop/src/index.html",
            "main": "apps/shop/src/main.ts",
            "polyfills": "apps/shop/src/polyfills.ts",
            "tsConfig": "apps/shop/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/shop/src/favicon.ico",
              "apps/shop/src/assets"
            ],
            "styles": [
              "apps/shop/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/shop/src/environments/environment.ts",
                  "with": "apps/shop/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "shop:build:production"
            },
            "development": {
              "browserTarget": "shop:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shop:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/shop/src/**/*.ts",
              "apps/shop/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/shop"
          ],
          "options": {
            "jestConfig": "apps/shop/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "ui",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/ui"
          ],
          "options": {
            "project": "libs/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui"
          ],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/src/**/*.ts",
              "libs/ui/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "counter"
}
