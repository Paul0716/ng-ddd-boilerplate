{
  "npmScope": "ng-nx-ddd-starter",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "counter": {
      "tags": []
    },
    "counter-e2e": {
      "tags": [],
      "implicitDependencies": ["counter"]
    },
    "domain": {
      "tags": []
    },
    "order": {
      "tags": []
    },
    "shared-state": {
      "tags": []
    },
    "shop": {
      "tags": []
    },
    "ui": {
      "tags": []
    }
  }
}
